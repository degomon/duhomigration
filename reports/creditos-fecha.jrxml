	<queryString>
		<![CDATA[select car.legacy_cartera_id as recordid,
bp.value as codigo, car.fecha,
car.fecha::date as fechaflat,
bp.name as nombrecliente,
car.monto, car.tasa, car.dias_cre as plazodias,
round(car.monto*car.tasa,2) as interes, car.montototal,
 u.name as usuario,
bpg.name as grupoTercero,
car.synced as sincronizado,
ru.name as nombreruta,
org.name as nombresucursal
from legacy_cartera car
inner join c_bpartner bp on car.c_bpartner_id = bp.c_bpartner_id
inner join ad_user u on car.createdby = u.ad_user_id
inner join c_bp_group bpg on bp.c_bp_group_id = bpg.c_bp_group_id
inner join cv_ruta ru on bp.cv_ruta_id = ru.cv_ruta_id
inner join ad_org org on bp.ad_org_trx_id = org.ad_org_id
where car.fecha::date between $P{fecha}::date and $P{fechafinal}::date
and bp.ad_org_trx_id = $P{orgid}
order by ru.name, car.created asc]]>
	</queryString>
