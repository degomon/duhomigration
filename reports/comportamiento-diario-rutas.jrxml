	<parameter name="fechafinal" class="java.util.Date"/>
	<parameter name="ad_org_id" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select m.caja,
m.fecha,
sum( case when m.tipomov = 'Cobro' then m.monto else 0.00 end) as cobros,
sum( case when m.tipomov = 'Desembolso' then m.monto else 0.00 end)  as desembolsos,
sum( case when m.tipomov = 'Gasto' then m.monto else 0.00 end) as gastos,
sum( case when m.tipomov = 'Cobro' then m.monto else 0.00 end) -
sum( case when m.tipomov = 'Desembolso' then m.monto else 0.00 end)  -
sum( case when m.tipomov = 'Gasto' then m.monto else 0.00 end) as saldo
from aad_pmovs m
inner join c_bankaccount bac on m.c_bankaccount_id = bac.c_bankaccount_id
where m.fecha between $P{fechainicial}::date and $P{fechafinal}::date
and bac.description = 'COBRADOR'
and m.ad_org_id = $P{ad_org_id}
group by m.caja, m.fecha
order by m.fecha, m.caja
---- garbage
---- garbage
---- garbage
---- garbage]]>
