	<queryString>
		<![CDATA[select m.codsucursal,
m.sucursal,
m.fecha,
sum( case when m.tipomov = 'Cobro' then m.monto else 0.00 end) as cobros,
sum( case when m.tipomov = 'Desembolso' then m.monto else 0.00 end)  as desembolsos,
sum( case when m.tipomov = 'Gasto' then m.monto else 0.00 end) as gastos,
sum( case when m.tipomov = 'Cobro' then m.monto else 0.00 end) -
sum( case when m.tipomov = 'Desembolso' then m.monto else 0.00 end)  -
sum( case when m.tipomov = 'Gasto' then m.monto else 0.00 end) as saldo
from aad_pmovs m
where fecha between $P{fechainicial}::date and $P{fechafinal}
group by m.codsucursal, m.fecha, m.sucursal
order by m.fecha, m.codsucursal, m.sucursal]]>
	</queryString>
