	<queryString>
		<![CDATA[select
cob.operacion::date as fecha,
cob.legacy_cobro_id,
'Ingresos Financieros clientes varios' as concepto,
cob.abono as bruto,
round( (cob.abono * 0.85), 2) as neto,
round( ( (cob.abono * 0.85) * 0.13), 2) as intereses,
round( ( (cob.abono * 0.85) * 0.02), 2) as comision,
round( ( (cob.abono * 0.85) * 0.15), 2) as ingresosfinancieros,
cp.startdate,
cp.enddate,
cp.name as periodname
from c_period cp
inner join legacy_cobro cob
on cob.operacion::date between cp.startdate::date and cp.enddate::date
inner join c_bpartner bp on cob.c_bpartner_id = bp.c_bpartner_id
where cp.c_period_id = $P{period_id}
and bp.c_greeting_id = $P{greeting_id} OR coalesce($P{greeting_id}, 1) = 1
-- group by cob.operacion::date
order by cob.operacion::date asc]]>
	</queryString>
