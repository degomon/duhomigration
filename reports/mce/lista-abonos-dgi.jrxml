	<queryString>
		<![CDATA[select bp.value as codigo,
bp.name as nombrecliente,
car.legacy_cartera_id as creditoid,
car.montototal,
cob.operacion::date as fechacobro,
cob.abono,
cp.name as periodo
from c_bpartner bp
inner join legacy_cobro cob
	on bp.c_bpartner_id = cob.c_bpartner_id
inner join c_period cp
	on cob.operacion::date between cp.startdate::date and cp.enddate::date
inner join legacy_cartera car on cob.id_cartera = car.legacy_cartera_id
where cp.c_period_id = $P{period_id}
and bp.c_greeting_id = $P{greeting_id} OR coalesce($P{greeting_id}, 1) = 1
order by bp.name, fechacobro asc
]]>
	</queryString>
